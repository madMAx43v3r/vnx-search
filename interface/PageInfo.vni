package vnx.search;

class PageInfo {
	
	uint id;						// page id (0 = not loaded yet)
	
	bool is_deleted;
	uchar engine_version;
	
	ulong index_version;			// version regarding title, etc
	ulong link_version;				// version regarding links
	ulong word_version;				// word context version
	
	long first_seen;
	long last_modified;
	
	string scheme;
	string title;
	
	vector<uint> words;
	vector<uint> links;
	vector<uint> reverse_links;
	
}
